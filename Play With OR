#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n];
	    int arr2[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    for(int i=0;i<n;i++){
	        if(i==n-1){
	            arr2[i]=arr[i];
	            cout<<arr2[i]<<" ";
	        }
	        else{
	            arr2[i]= arr[i]|arr[i+1];
	            cout<<arr2[i]<<" ";
	        }
	    }   
	    cout<<endl;
	}
	return 0;
}
