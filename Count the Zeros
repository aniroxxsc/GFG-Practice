#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n,count=0;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    for(int i=n-1;i>=0;i--){
	        if(arr[i]==0){
	            count++;
	        }
	        else if(arr[i]==1){
	            break;
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}
