#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n],sum_even=0,sum_odd=0;
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	        if(arr[i]%2==0){
	            sum_even++;
	        }
	        else{
	            sum_odd++;
	        }
	    }
	    cout<<sum_even*sum_odd<<endl;
	}
	return 0;
}
