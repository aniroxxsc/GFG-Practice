#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    long long max=0,min= 1000000000001;
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	        if(arr[i]>max){
	            max=arr[i];
	        }
	        if(arr[i]<min){
	            min=arr[i];
	        }
	    }
	    cout<<min<<" "<<max<<endl;
	}
	return 0;
}
