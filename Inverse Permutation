#include <iostream>
using namespace std;

int main() {
	int t;
	int n;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    int arr2[n];
	    for(int i=0;i<n;i++){
	        arr2[arr[i]-1]=i+1;
	    }
	    for(int j=0;j<n;j++){
	        cout<<arr2[j]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
